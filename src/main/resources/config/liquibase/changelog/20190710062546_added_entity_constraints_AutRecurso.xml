    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity AutRecurso.
    -->
    <changeSet id="20190710062546-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="id_recurso_dominio_id"
